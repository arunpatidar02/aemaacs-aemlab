<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/aemlab/oneweb/settings/wcm/templates/page-content"
        jcr:primaryType="cq:PageContent"
        jcr:title="Adpative Image"
        sling:resourceType="aemlab/oneweb/components/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemlab/oneweb/components/container">
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemlab/oneweb/components/container">
                <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemlab/oneweb/components/container"
                    layout="responsiveGrid">
                    <title
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Adpative Image Example"
                        sling:resourceType="aemlab/oneweb/components/title"/>
                    <adaptive-image
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemlab/oneweb/concept/components/adaptive-image"
                        fileReference="/content/dam/aemlab/oneweb/banners/banner-1.png"
                        titleStyle = "b"
                        title="This is an example of radio initialization and using core image">
                        <image 
                            jcr:primaryType="nt:unstructured"
                            fileReference="/content/dam/aemlab/oneweb/banners/banner-2.png"
                            sling:resourceType="aemlab/oneweb/components/image"/>
                    </adaptive-image>
                </container>
            </container>
        </root>
    </jcr:content>
</jcr:root>
