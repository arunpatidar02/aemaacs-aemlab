<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/aemlab/oneweb/settings/wcm/templates/page-content"
        jcr:primaryType="cq:PageContent"
        jcr:title="Tag Picker"
        sling:resourceType="aemlab/oneweb/components/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemlab/oneweb/components/container">
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemlab/oneweb/components/container">
                <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemlab/oneweb/components/container"
                    layout="responsiveGrid">
                    <tag-picker
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemlab/oneweb/concept/components/tag-picker"
                        tag="/content/dam/aemlab/oneweb/banners/banner-1.png"
                        tag2="[experience-fragments:variation/web,experience-fragments:variation/facebook,experience-fragments:variation/pinterest]"
                        tag3="experience-fragments:variation/facebook"/>
                </container>
            </container>
        </root>
    </jcr:content>
</jcr:root>
