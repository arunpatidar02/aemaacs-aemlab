<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[/etc/mobile/groups/responsive]"
        cq:template="/conf/aemlab/settings/wcm/templates/page-content"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="aemlab/components/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemlab/components/container"
            layout="simple">
            <header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemlab/concept/components/cac-xf"
                tagName="header"
                classList="[aemlab-header, aemlab-header__cac]"
                configName="com.community.aemlab.core.conf.HeaderFooterConfig"
                property="headerPath"
                noConfigLabel="No configuration defined for Header"/>
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemlab/components/container"
                layout="responsiveGrid">
                <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemlab/components/container"
                    editable="{Boolean}true"/>
            </container>
            <go_to_top
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemlab/concept/components/go-to-top"/>
            <footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemlab/concept/components/cac-xf"
                tagName="footer"
                classList="[aemlab-footer, aemlab-footer__cac]"
                configName="com.community.aemlab.core.conf.HeaderFooterConfig"
                property="footerPath"
                noConfigLabel="No configuration defined for Footer"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}768"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
